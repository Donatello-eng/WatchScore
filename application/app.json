{
  "expo": {
    "name": "WatchScore",
    "slug": "WatchScore",
    "scheme": "watchscore",  
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#461647"
    },
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "com.bumpgames.watchscore",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "package": "com.bumpgames.watchscore",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-icon-back.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
